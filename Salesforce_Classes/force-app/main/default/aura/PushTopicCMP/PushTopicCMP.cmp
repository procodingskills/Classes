<aura:component implements="flexipage:availableForAllPageTypes">
    <lightning:empApi aura:id="empApi"/>
    <aura:handler name="init" value="{!this}" action="{!c.initHandler}"/>
    <aura:attribute name="eventData" type="String"/>
    <aura:attribute name="channelSub" type="String"/>
    EventData : {!v.eventData}<br/>
    channelSub : {!v.channelSub}<br/>
    <lightning:button label="Subscribe" onclick="{!c.subscribeEmpApi}"/>
</aura:component>	
